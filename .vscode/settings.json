{
  "workbench.colorTheme": "Solarized Dark",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true
  },
  "hide-files.files": [],
  "liveServer.settings.port": 5500,
  "liveServer.settings.useWebExt": true,
  "liveServer.settings.proxy": {
      "enable": true,
      "baseUri": "/",
      "proxyUri": "http://localhost:8888/theater-remote/"
  },
}